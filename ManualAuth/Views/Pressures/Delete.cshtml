@model ManualAuth.Models.Pressure

@{
    ViewData["Title"] = "Delete";
}

<h3>Are you sure you want to delete this result?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Date and time
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PressureDateTime)
        </dd>
        <dt class="col-sm-2">
            Systolic pressure
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PressureSYSValue)
        </dd>
        <dt class="col-sm-2">
            Diastolic Pressure
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PressureDIAValue)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
