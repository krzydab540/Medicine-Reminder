@model ManualAuth.Models.Pressure

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<h5>Pressure</h5>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Date and time
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PressureDateTime)
        </dd>
        <dt class="col-sm-2">
            Systolic value
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PressureSYSValue)
        </dd>
        <dt class="col-sm-2">
            Diastolic value
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PressureDIAValue)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>